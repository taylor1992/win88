<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.company.project.mapper.ExpenseMapper">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.company.project.entity.ExpenseEntity" id="expenseMap">
        <result property="id" column="id"/>
        <result property="expenseDate" column="expense_date"/>
        <result property="category" column="category"/>
        <result property="amount" column="amount"/>
        <result property="currency" column="currency"/>
        <result property="bizTag" column="biz_tag"/>
        <result property="orderNo" column="order_no"/>
        <result property="remark" column="remark"/>
        <result property="status" column="status"/>
        <result property="deleted" column="deleted"/>
        <result property="createdBy" column="created_by"/>
        <result property="createdTime" column="created_time"/>
        <result property="updatedBy" column="updated_by"/>
        <result property="updatedTime" column="updated_time"/>
    </resultMap>


</mapper>