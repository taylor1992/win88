<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.company.project.mapper.ProductMapper">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.company.project.entity.ProductEntity" id="productMap">
        <result property="id" column="id"/>
        <result property="productCode" column="product_code"/>
        <result property="productName" column="product_name"/>
        <result property="productNameEn" column="product_name_en"/>
        <result property="brandName" column="brand_name"/>
        <result property="categoryId" column="category_id"/>
        <result property="mainImageUrl" column="main_image_url"/>
        <result property="thumbnailImageUrl" column="thumbnail_image_url"/>
        <result property="shortDescription" column="short_description"/>
        <result property="detailDescription" column="detail_description"/>
        <result property="status" column="status"/>
        <result property="remark" column="remark"/>
        <result property="createdBy" column="created_by"/>
        <result property="updatedBy" column="updated_by"/>
        <result property="createdTime" column="created_time"/>
        <result property="updatedTime" column="updated_time"/>
    </resultMap>


</mapper>